{
	"name": "cronsite-2020",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"bootstrap": "^4.4.1",
		"react": "^16.12.0",
		"react-bootstrap": "^1.0.0-beta.16",
		"react-dom": "^16.12.0",
		"react-feather": "^2.0.3",
		"react-scripts": "3.4.0",
		"typescript": "^3.7.5"
	},
	"devDependencies": {
		"@types/node": "^12.12.27",
		"@types/react": "^16.9.19",
		"@types/react-dom": "^16.9.5",
		"husky": "^4.2.3",
		"lint-staged": "^10.0.7",
		"node-sass": "^4.13.1",
		"prettier": "^1.19.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.22%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"prettier": {
		"jsxBracketSameLine": true,
		"printWidth": 140,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 2,
		"useTabs": true
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write",
			"git add"
		]
	}
}
